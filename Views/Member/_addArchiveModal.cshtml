<!-- Modal-->
<div class="modal fade" id="addCancellationModal" tabindex="-1" role="dialog" aria-labelledby="SpecialtymyModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="SpecialtymyModalLabel">Member Archiving</h4>
                <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body">
                <input type="hidden" id="MemberID" name="MemberID" />

                @{
                    var s = new Cancellation ();
                    <partial name="_MemberCancellation" model="s" />
                }
            </div>
        </div>
    </div>
</div>